{
    "name": "laravel\/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/exodusanto/critical-css"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel\/framework": "5.5.*",
        "mcms\/core-package": "0.*",
        "mcms\/package-admin": "0.*",
        "mcms\/package-pages": "0.*",
        "mcms\/front-end": "0.*",
        "guzzlehttp\/guzzle": "^6.2",
        "barryvdh\/laravel-translation-manager": "dev-master",
        "mbouclas\/mcms-mailchimp": "1.*",
        "krisawzm/critical-css": "dev-laravel-55",
        "laravel/scout": "^3.0",
        "laravel/tinker": "^1.0",
        "algolia/algoliasearch-client-php": "^1.20"
    },
    "require-dev": {
        "fzaninotto\/faker": "~1.4",
        "mockery\/mockery": "0.9.*",
        "phpunit\/phpunit": "~5.0",
        "symfony\/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app\/",
            "FrontEnd\\": "FrontEnd\/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests\/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",

            "php artisan core:refreshAssets",
            "php artisan admin:refreshAssets",
            "php artisan frontend:refreshAssets",
            "php artisan pages:refreshAssets",
            "php artisan mcmsMailchimp:refreshAssets"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
